{
    "extends": ["eslint:recommended",       "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended", "plugin:react/recommended", "plugin:prettier/recommended"],
    "rules": {
        "react/react-in-jsx-scope": "off"
      },
      "env": {
        "browser": true,
        "es2021": true,
        "jest": true
      },
      "parserOptions": {
        "sourceType": "module"
    },
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ]

}